{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "rootDir": "src",
    "outDir": "dist",
    "target": "es5",
    "module": "ESNext",
    "lib": ["dom", "dom.iterable", "esnext"],
    "jsx": "react-jsx",
    "baseUrl": "src",
    "paths": {
      "@components/*": ["./components/*"],
      "@utils": ["./utils"],
      "@hooks": ["./hooks"],
      "@store/*": ["./store/*"]
    }
  },
  "include": ["src"]
}
