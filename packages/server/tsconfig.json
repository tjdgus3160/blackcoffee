{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "rootDir": "src",
    "outDir": "dist",
    "target": "es5",
    "module": "CommonJS",
    "baseUrl": ".",
    "noUnusedParameters": false,
    "noImplicitReturns": true,
    "noImplicitAny": false,
    "paths": {
      "@types": ["./src/@types"]
    }
  },
  "include": ["src/**/*"]
}
